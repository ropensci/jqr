LIBJQ = \
	jq/builtin.o \
	jq/bytecode.o \
	jq/compile.o \
	jq/execute.o \
	jq/jv_alloc.o \
	jq/jv_aux.o \
	jq/jv_dtoa.o \
	jq/jv_file.o \
	jq/jv_parse.o \
	jq/jv_print.o \
	jq/jv_unicode.o \
	jq/jv.o \
	jq/lexer.o \
	jq/locfile.o \
	jq/parser.o

$(SHLIB): libjq.a

libjq.a: $(LIBJQ)
	$(AR) rcs libjq.a $(LIBJQ)

PKG_CPPFLAGS = -Ijq
PKG_LIBS = -L. -ljq
